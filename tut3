import React from "react";

import ReactDOM from "react-dom";

const myname="Arvind Kumar Sahu"
const fname="Arvind"
const lname="Sahu"
ReactDOM.render(
  <>
    <h1> My name is {myname} </h1>
    <p> My lucky number is {3+4} </p>

    <p> Generating Random Number is {Math.random()}</p>

    
    <h1> Printing my name using Template literals</h1>
<p> {` My anme using template literals ${fname} ${lname}`}</p>
  </>,
  document.getElementById("root")
)
